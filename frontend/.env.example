# React App Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_APP_NAME=StorySentinel
REACT_APP_VERSION=1.0.0

# Story Protocol Configuration
REACT_APP_STORY_PROTOCOL_NETWORK=testnet
REACT_APP_STORY_PROTOCOL_CHAIN_ID=1516
REACT_APP_STORY_PROTOCOL_RPC_URL=https://rpc.odyssey.storyrpc.io

# Blockchain Configuration
REACT_APP_ETHEREUM_CHAIN_ID=1
REACT_APP_ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-api-key
REACT_APP_POLYGON_CHAIN_ID=137
REACT_APP_POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-api-key

# Wallet Configuration
REACT_APP_WALLET_CONNECT_PROJECT_ID=your-wallet-connect-project-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DISPUTES=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services
REACT_APP_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/
REACT_APP_SENTRY_DSN=your-sentry-dsn-for-error-tracking

# Development Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug
